let
  var a : Integer;
  var b : Integer;
  var c : Integer;
  var d : Integer
in
begin
  a := 0;
  b := 1;
  c := 2;
  d := 2;
  while a < 5 do
  begin
    put('a');
    a := a + 1;
    d := d + 2;
    d := d + 3;
    d := d + 4;
    d := d + 5;
    d := d + 6;
    d := d + 7;
    d := d + 8;
    d := d + 9;
    d := d + 10;
    b := c + 2; ! this could be hoisted
  end;

  puteol();
  putint(a);
  puteol();
  putint(b);
  puteol();
  putint(c);
  puteol();
end